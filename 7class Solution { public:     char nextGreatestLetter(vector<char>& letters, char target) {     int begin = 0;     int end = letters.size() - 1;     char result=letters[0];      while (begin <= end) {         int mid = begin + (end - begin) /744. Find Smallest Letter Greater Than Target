class Solution {
public:
    char nextGreatestLetter(vector<char>& letters, char target) {
    int begin = 0;
    int end = letters.size() - 1;
    char result=letters[0];

    while (begin <= end) {
        int mid = begin + (end - begin) / 2; 

        if (letters[mid] > target){
            result=letters[mid];
            end = mid - 1;}
        else
            begin=mid+1;
    }

    return result;
        
    }
};
